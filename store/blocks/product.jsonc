{
  "store.product": {
    "children": [
      "breadcrumb",
      "flex-layout.row#main"
    ]
  },

  "flex-layout.row#main": { 
    "props": { 
      "marginTop": 6
    },
    "children": [
      "flex-layout.col#left",
      "flex-layout.col#right"
    ]
  },

  "flex-layout.col#left": {
  "children": [
      "product-images"
    ]
  },
  
  "flex-layout.col#right":{
    "props": {
      "verticalAlign": "middle",
      "preventVerticalStretch":"true"
    },
    "children":[
      "product-name",
      "product-identfier.product",
      "product-price",
      "buy-button",
      "shipping-simulator"
      
    ]
  },

  "flex-layout.row#qty-sku": {
    "children": [
      "sku-selector",
      "product-quantity"
      
    ]
  },

  
  "sku-selector": {
    "props": {
      "hidelmpossibleCombinations": "false"
    }
  },

  "product-quantity": {
    "props": {
      "warningQuantityThreshold": 9999999
    }
  },

  "shipping-simulator" :{
    "props": {
      "skulD": "342"
    }
  },

  "product-price": {
    "props": {
      "showSavings": "true",
      "showListPrice": true
    }
  }
}